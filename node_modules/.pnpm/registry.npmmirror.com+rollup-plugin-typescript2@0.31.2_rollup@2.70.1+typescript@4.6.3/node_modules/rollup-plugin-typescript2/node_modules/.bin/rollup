#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+rollup@2.70.1/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+rollup@2.70.1/node_modules/rollup/dist/bin/rollup" "$@"
fi
