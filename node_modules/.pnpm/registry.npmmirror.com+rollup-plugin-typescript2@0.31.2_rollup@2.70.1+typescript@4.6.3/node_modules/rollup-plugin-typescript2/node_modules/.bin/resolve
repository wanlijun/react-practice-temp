#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
fi
