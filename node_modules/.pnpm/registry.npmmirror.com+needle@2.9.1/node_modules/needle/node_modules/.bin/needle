#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lijunwan/caicai/project/best/react-practice-temp/node_modules/.pnpm/registry.npmmirror.com+needle@2.9.1/node_modules/needle/bin/node_modules:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules/.pnpm/registry.npmmirror.com+needle@2.9.1/node_modules/needle/node_modules:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules/.pnpm/registry.npmmirror.com+needle@2.9.1/node_modules:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules/.pnpm/node_modules:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules:/home/lijunwan/caicai/project/best/node_modules:/home/lijunwan/caicai/project/node_modules:/home/lijunwan/caicai/node_modules:/home/lijunwan/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules/.pnpm/registry.npmmirror.com+needle@2.9.1/node_modules/needle/bin/node_modules:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules/.pnpm/registry.npmmirror.com+needle@2.9.1/node_modules/needle/node_modules:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules/.pnpm/registry.npmmirror.com+needle@2.9.1/node_modules:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules/.pnpm/node_modules:/home/lijunwan/caicai/project/best/react-practice-temp/node_modules:/home/lijunwan/caicai/project/best/node_modules:/home/lijunwan/caicai/project/node_modules:/home/lijunwan/caicai/node_modules:/home/lijunwan/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/needle" "$@"
else
  exec node  "$basedir/../../bin/needle" "$@"
fi
